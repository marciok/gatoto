  Landed by HTML5 UP
  html5up.net | @ajlkn
  Free for personal and commercial use under the CCA 3.0 license (html5up.net/license)
%html
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %title Landed by HTML5 UP
    %meta{:charset => "utf-8"}/
    %meta{:content => "width=device-width, initial-scale=1", :name => "viewport"}/
    /[if lte IE 8] <script src="assets/js/ie/html5shiv.js"></script>
    %link{:href => "assets/css/main.css", :rel => "stylesheet"}/
    /[if lte IE 9] <link rel="stylesheet" href="assets/css/ie9.css" />
    /[if lte IE 8] <link rel="stylesheet" href="assets/css/ie8.css" />
  %body.landing
    #page-wrapper
      / Header
      %header#header
        %h1#logo
          %a{:href => "index.html"} Landed
        %nav#nav
          %ul
            %li
              %a{:href => "index.html"} Home
            %li
              %a{:href => "#"} Layouts
              %ul
                %li
                  %a{:href => "left-sidebar.html"} Left Sidebar
                %li
                  %a{:href => "right-sidebar.html"} Right Sidebar
                %li
                  %a{:href => "no-sidebar.html"} No Sidebar
                %li
                  %a{:href => "#"} Submenu
                  %ul
                    %li
                      %a{:href => "#"} Option 1
                    %li
                      %a{:href => "#"} Option 2
                    %li
                      %a{:href => "#"} Option 3
                    %li
                      %a{:href => "#"} Option 4
            %li
              %a{:href => "elements.html"} Elements
            %li
              %a.button.special{:href => "#"} Sign Up
      / Banner
      %section#banner
        .content
          %header
            %h2=I18n.t(:project)[:title]
            %p
              = I18n.t(:project)[:description_1]
              %br/
              = I18n.t(:project)[:description_2]
          %span.image
            %img{:alt => "", :src => "images/pic01.jpg"}/
        %a.goto-next.scrolly{:href => "#one"}=I18n.t(:next)
      / One
      %section#one.spotlight.style1.bottom
        %span.image.fit.main
          %img{:alt => "", :src => "images/pic02.jpg"}/
        .content
          .container
            .row
              .4u{:class => "12u$(medium)"}
                %header
                  %h2=I18n.t(:details)[:one][:title]
                  %p=I18n.t(:details)[:one][:second_title]
              .4u{:class => "12u$(medium)"}
                %p=I18n.t(:details)[:one][:description_1]
              %div{:class => "4u$ 12u$(medium)"}
                %p=I18n.t(:details)[:one][:description_2]
        %a.goto-next.scrolly{:href => "#two"}=I18n.t(:next)
      / Two
      %section#two.spotlight.style2.right
        %span.image.fit.main
          %img{:alt => "", :src => "images/pic03.jpg"}/
        .content
          %header
            %h2=I18n.t(:details)[:two][:title]
            %p=I18n.t(:details)[:two][:second_title]
          %p=I18n.t(:details)[:two][:description]
          %ul.actions
            %li
              %a.button{:href => "#"}=I18n.t(:learn_more)
        %a.goto-next.scrolly{:href => "#three"}=I18n.t(:next)
      / Three
      %section#three.spotlight.style3.left
        %span.image.fit.main.bottom
          %img{:alt => "", :src => "images/pic04.jpg"}/
        .content
          %header
            %h2=I18n.t(:details)[:three][:title]
            %p=I18n.t(:details)[:three][:second_title]
          %p=I18n.t(:details)[:three][:description]
          %ul.actions
            %li
              %a.button{:href => "#"}=I18n.t(:learn_more)
        %a.goto-next.scrolly{:href => "#four"}=I18n.t(:next)
      / Four
      %section#four.wrapper.style1.special.fade-up
        .container
          %header.major
            %h2=I18n.t(:details)[:four][:title]
            %p=I18n.t(:details)[:four][:second_title]
          .box.alt
            .row.uniform
              %section.4u{:class => "6u(medium) 12u$(xsmall)"}
                %span.icon.alt.major.fa-area-chart
                %h3=I18n.t(:details)[:four][:boxes][:one][:title]
                %p=I18n.t(:details)[:four][:boxes][:one][:description]
              %section.4u{:class => "6u$(medium) 12u$(xsmall)"}
                %span.icon.alt.major.fa-comment
                %h3=I18n.t(:details)[:four][:boxes][:two][:title]
                %p=I18n.t(:details)[:four][:boxes][:two][:description]
              %section{:class => "4u$ 6u(medium) 12u$(xsmall)"}
                %span.icon.alt.major.fa-flask
                %h3=I18n.t(:details)[:four][:boxes][:three][:title]
                %p=I18n.t(:details)[:four][:boxes][:three][:description]
              %section.4u{:class => "6u$(medium) 12u$(xsmall)"}
                %span.icon.alt.major.fa-paper-plane
                %h3=I18n.t(:details)[:four][:boxes][:four][:title]
                %p=I18n.t(:details)[:four][:boxes][:four][:description]
              %section.4u{:class => "6u(medium) 12u$(xsmall)"}
                %span.icon.alt.major.fa-file
                %h3=I18n.t(:details)[:four][:boxes][:five][:title]
                %p=I18n.t(:details)[:four][:boxes][:five][:description]  
              %section{:class => "4u$ 6u$(medium) 12u$(xsmall)"}
                %span.icon.alt.major.fa-lock
                %h3=I18n.t(:details)[:four][:boxes][:six][:title]
                %p=I18n.t(:details)[:four][:boxes][:six][:description]
          %footer.major
            %ul.actions
              %li
                %a.button{:href => "#"}=I18n.t(:details)[:four][:action]
      / Five
      %section#five.wrapper.style2.special.fade
        .container
          %header
            %h2=I18n.t(:details)[:five][:title]
            %p=I18n.t(:details)[:five][:description]
          %form.container{:action => "#", :class => "50%", :method => "post"}
            .row.uniform{:class => "50%"}
              .8u{:class => "12u$(xsmall)"}
                %input#email{:name => "email", :placeholder => I18n.t(:email_address), :type => "email"}/
              %div{:class => "4u$ 12u$(xsmall)"}
                %input.fit.special{:type => "submit", :value => I18n.t(:get_started) }/
      / Footer
      %footer#footer
        %ul.icons
          %li
            %a.icon.alt.fa-twitter{:href => "#"}
              %span.label Twitter
          %li
            %a.icon.alt.fa-facebook{:href => "#"}
              %span.label Facebook
          %li
            %a.icon.alt.fa-linkedin{:href => "#"}
              %span.label LinkedIn
          %li
            %a.icon.alt.fa-instagram{:href => "#"}
              %span.label Instagram
          %li
            %a.icon.alt.fa-github{:href => "#"}
              %span.label GitHub
          %li
            %a.icon.alt.fa-envelope{:href => "#"}
              %span.label Email
        %ul.copyright
          %li Â© Gatoto. All rights reserved.
          %li
            Design:
            %a{:href => "http://html5up.net"} HTML5 UP
    / Scripts
    %script{:src => "assets/js/jquery.min.js"}
    %script{:src => "assets/js/jquery.scrolly.min.js"}
    %script{:src => "assets/js/jquery.dropotron.min.js"}
    %script{:src => "assets/js/jquery.scrollex.min.js"}
    %script{:src => "assets/js/skel.min.js"}
    %script{:src => "assets/js/util.js"}
    /[if lte IE 8] <script src="assets/js/ie/respond.min.js"></script>
    %script{:src => "assets/js/main.js"}

